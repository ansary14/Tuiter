<div id="chatroom-container" class="row, bg-dark text-light">
  <div id="side-panel" class="col-md-4 col-sm-4">
    <h5> Hi <%= current_user.email %></h5>
    <h4>Users</h4>
    <div>
      <%= turbo_stream_from "users" %>
      <div id="users">
        <%= render @users %>
      </div>
    </div>
    <h4>Rooms</h4>
    <%= render partial: 'layouts/new_room_form' %>
    <div>
      <%= turbo_stream_from "rooms" %>
      <div id="rooms">
        <%= render @rooms %>
      </div>
    </div>
  </div>

  <div id="room-container" class="col-md-8 col-sm-8 bg-dark text-light">
    Hi
  </div>
</div>
